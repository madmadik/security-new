<Page x:Class="WpfApp8.Delete"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp8"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Delete">

    <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
        <Button Content="Удалить"  Click="Button_Delete" HorizontalAlignment="Left" Margin="208,266,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock HorizontalAlignment="Left" Margin="43,67,0,0" TextWrapping="Wrap" Text="Введите id" VerticalAlignment="Top"/>
        <TextBox Name="id" HorizontalAlignment="Left" Height="23" Margin="43,98,0,0" TextWrapping="Wrap"   VerticalAlignment="Top" Width="120"/>
        <Button Content="Назад" Click="Button_Back"  HorizontalAlignment="Left" Margin="26,266,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Выйти" Click="Button_Exit" HorizontalAlignment="Left" Margin="368,267,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Page>
